@{
}

@model IEnumerable<string>

<div class="d-block">
    <a asp-controller="Home" asp-action="Index" asp-route-bookCategory="" asp-route-pageNum="1" class="btn d-flex btn-dark">Home</a><br />

    @foreach (string b in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-bookCategory="@b" asp-route-pageNum="1"
           class="btn btn-dark d-flex @(ViewBag.SelectedCategory == b ? "text-decoration-underline" : "")">@b</a><br />
    }
</div>